{
    "name": "codestitch-kit",
    "version": "2.0.1",
    "description": "Russified version of the official CodeStitch Intermediate kit, featuring 11ty, Decap CMS and SASS.",
    "main": "index.js",
    "scripts": {
        "watch:cms": "npx decap-server",
        "watch:eleventy": "cross-env ELEVENTY_ENV=DEV eleventy --serve",
        "build:eleventy": "cross-env ELEVENTY_ENV=PROD eleventy",
        "start": "run-p watch:*",
        "build": "run-s build:*",
        "preview": "cross-env ELEVENTY_ENV=PROD eleventy --serve"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/m-melnikov/codestitch-kit.git"
    },
