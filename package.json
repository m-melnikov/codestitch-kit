    "bugs": {
        "url": "https://github.com/m-melnikov/codestitch-kit/issues"
    },
    "homepage": "https://github.com/m-melnikov/codestitch-kit#readme",
    "dependencies": {
        "@11ty/eleventy": "^3.1.2",
        "@codestitchofficial/eleventy-plugin-minify": "^1.1.3",
        "@codestitchofficial/eleventy-plugin-sharp-images": "^2.2.0",
        "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
        "autoprefixer": "^10.4.22",
        "codestitch-sharp-image-automation": "^0.4.0",
        "cross-env": "^10.1.0",
        "cssnano": "^7.1.2",
        "decap-server": "^3.4.0",
        "esbuild": "^0.27.0",
        "glob": "^13.0.0",
